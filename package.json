{
  "name": "@NewYorkStyle/project-template-ui",
  "version": "0.0.1",
  "description": "React UI Kit for Project Template",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "semantic-release": "semantic-release",
    "semantic-release:dry-run": "semantic-release --dry-run",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,md,mdx,css,yaml}\"",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,js,md,mdx,css,yaml}\""
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css,scss}": [
      "prettier --write"
    ]
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "antd": "^6",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "devDependencies": {
    "@eslint/js": "9.39.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.6",
    "@semantic-release/npm": "12.0.2",
    "@storybook/addon-a11y": "10.2.12",
    "@storybook/addon-docs": "10.2.12",
    "@storybook/addon-webpack5-compiler-swc": "4.0.2",
    "@storybook/react-webpack5": "10.2.12",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.56.1",
    "@typescript-eslint/parser": "8.56.1",
    "antd": "6",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "css-loader": "6.8.1",
    "eslint": "9.39.3",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-sort-destructure-keys": "3.0.0",
    "eslint-plugin-storybook": "10.2.12",
    "globals": "17.3.0",
    "husky": "8.0.3",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "lint-staged": "15.2.10",
    "prettier": "3.6.2",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "sass": "1.80.5",
    "sass-loader": "16.0.3",
    "semantic-release": "24.2.9",
    "storybook": "10.2.12",
    "style-loader": "3.3.3",
    "style-resources-loader": "1.5.0",
    "ts-jest": "29.4.6",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1"
  }
}
